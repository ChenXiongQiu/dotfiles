// Place your key bindings in this file to overwrite the defaults
[
 { "key": "cmd+shift+k",         "command": "workbench.action.nextEditor" },
 { "key": "cmd+shift+j",         "command": "workbench.action.previousEditor" },
 // TODO: : doesn't work
{ "key": "ctrl + j",        "command": "workbench.action.focusPreviousGroup" },
{ "key": "ctrl + k",        "command": "workbench.action.focusNextGroup" },
  { "key": "Space w",  "command": "workbench.action.files.save", "when": "editorFocus && !inDebugRepl && vim.mode == 'Normal Mode'" },
  {"key": "ctrl+ f",       "command":"extension.dash.specific"},
  {"key":  "cmd+m", "command":"code-runner.run" },
  {
    "key": "cmd+;",
    "command": "editor.action.commentLine"
  },
  {
    "key": "shift+cmd+i",
    "command": "git.push"
  }
]




