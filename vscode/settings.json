        {
          "command": "workbench.action.files.save",
          "args": []
        }
      ]
    },
    {
      "before": [
        "K"
      ],
      "after": [
        "leader",
        "leader",
        "k"
      ]
    },
    {
      "before": [
        "J"
      ],
      "after": [
        "leader",
        "leader",
        "j"
      ]
    },
    {
      "before": [
        "<leader>",
        "i"
      ],
      "after": [
        "y",
        "i",
        "\""
      ]
    },
    {
      "before": [
        "Y"
      ],
      "after": [
        "y",
        "$"
      ]
    },
    {
      "before": [
        "<leader>",
        "e"
      ],
      "after": [],
      "commands": [
        {
          "command": "editor.action.commentLine",
          "args": []
