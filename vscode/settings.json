    "godoc"
  ],
  "editor.lineNumbers": "relative",
  "editor.cursorStyle": "line",
  "explorer.confirmDragAndDrop": false,
  "window.zoomLevel": -2,
  "workbench.welcome.enabled": false,
  // _Python
  "python.linting.pylintArgs": [
    "--errors-only"
