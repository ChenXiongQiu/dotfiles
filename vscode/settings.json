  "todohighlight.isEnable": true,
  "editor.cursorBlinking": "solid",
  "files.trimTrailingWhitespace": true,
  "todohighlight.isCaseSensitive": true,
  "todohighlight.keywords": [
    {
      "text": "NOTE:",
      "color": "yellow",
      "backgroundColor": "rgba(0,0,0,.2)"
    },
    {
      "text": "TODO:",
      "color": "red",
      "backgroundColor": "rgba(0,0,0,.2)"
    },
    {
      "text": "FIXME:",
      "color": "orange",
      "backgroundColor": "rgba(0,0,0,.2)"
    }
