# my aliases, functions that I use for development purposes

# node 
alias n='node'
alias ndi='node install'

#npm
alias zi='npm install'
alias zs='npm start'
alias zl='npm ls'
alias zu='npm update'
alias zo='npm init'
alias zg='npm install -g'
alias zb='npm run build'

# go
alias o='go run'
alias og='go get'
alias oo='go build'

# meteor
alias mt='meteor'

# compile code
