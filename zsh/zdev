# my aliases, functions that I use for development purposes

# _dev
alias w='code'
alias w.='code .'

# _web
alias li='open -a Google\ Chrome\ Canary index.html'
alias bss='browser-sync'

# _node 
alias n='node'
alias ndi='node install'
alias n.='node .'

# _yarn
alias y='yarn'
alias yi='yarn init'
alias ya='yarn add'
alias yr='yarn remove'
alias yg='yarn global add'

# _npm 
alias zi='npm install'
alias zs='npm start'
alias zl='npm ls'
alias zu='npm update'
alias zo='npm init'
alias zg='npm install -g'
alias zb='npm run build'

# _go
alias o='go run'
alias oo='go get'
alias ob='go build'

# _meteor
alias mt='meteor'

# _compile code
alias sw='swift'

# _ruby
alias r='ruby'

