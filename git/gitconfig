	excludesfile = /Users/nikivi/.gitignore_global
[github]
    user = nikitavoloboev
[alias]
	cim = !git commit -m \"$*\" && :
	s = status
	d = diff
    open = !open `git config remote.origin.url`
    url =! bash -c 'git config --get remote.origin.url | sed -E "s/.+:\\(.+\\)\\.git$/https:\\\\/\\\\/github\\\\.com\\\\/\\\\1/g"'
